
public class TestList {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		MyLL theLL = new MyLL();
		
		StudentInfo firstStudent = new StudentInfo(111111, "Daffy", "Duck");
		theLL.addToRear(firstStudent);
		
		StudentInfo secondStudent = new StudentInfo(111112, "Raffy", "Ruck");
		theLL.addToRear(secondStudent);
		
		StudentInfo zeroethStudent = new StudentInfo(111110, "Caffy", "Buck");
		theLL.addToFront(zeroethStudent);
		
		theLL.displayList();
	}

}
